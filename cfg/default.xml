<?xml version="1.0"?>
<core>
    <network enabled="true">
        <interface>eth0</interface>
        <dhcp>false</dhcp>
        <netmask>255.255.255.0</netmask>
        <default_ip>192.168.0.10</default_ip>
    </network>
    <plugindir>.</plugindir>
    <module file="libdoor.so">
        <alias>doorA</alias>
        <properties>
            <doorconf>cfg/doorA.conf</doorconf>
            <buzzer>buzzerA</buzzer>
            <doorRelay>relayA</doorRelay>
            <doorSensor>sensor</doorSensor>
            <grantedLed>led1</grantedLed>
        </properties>
    </module>
    <module file="libjournal.so">
        <alias>log</alias>
        <properties/>
    </module>
    <module file="libledmonitor.so">
        <alias>monitor</alias>
        <properties>
            <led type="system" device="green"/>
        </properties>
    </module>
    <module file="libwiegand.so">
        <alias>reader</alias>
        <properties>
            <target>doorA</target>
            <readerDevice>readerA</readerDevice>
        </properties>
    </module>
    <module file="libauthtest.so">
        <alias>authtest</alias>
        <properties>
            <configpath>cfg/auth.conf</configpath>
        </properties>
    </module>
</core>
