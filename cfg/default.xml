<?xml version="1.0"?>
<core>
    <plugindir>.</plugindir>
    <module file="libdoor.so">
        <alias>doorA</alias>
        <properties>
            <doorconf>cfg/doorA.conf</doorconf>
            <buzzer>buzzerA</buzzer>
            <doorRelay>relayA</doorRelay>
            <doorSensor>sensor</doorSensor>
            <grantedLed>led1</grantedLed>
        </properties>
    </module>
    <module file="libdoor.so">
        <alias>doorB</alias>
        <properties>
            <doorconf>cfg/doorB.conf</doorconf>
            <doorRelay>none</doorRelay>
            <doorButton>none</doorButton>
            <grantedLed>none</grantedLed>
            <deniedLed>none</deniedLed>
        </properties>
    </module>
    <module file="libjournal.so">
        <alias>log</alias>
        <properties/>
    </module>
    <module file="libledmonitor.so">
        <alias>monitor</alias>
        <properties>
            <led type="system" device="green"/>
        </properties>
    </module>
    <module file="libwiegand.so">
        <alias>reader</alias>
        <properties>
            <target>doorA</target>
            <readerDevice>readerA</readerDevice>
        </properties>
    </module>
    <module file="libauthtest.so">
        <alias>authtest</alias>
        <properties>
            <configpath>cfg/auth.conf</configpath>
        </properties>
    </module>
</core>
